    Test1 : Test_ExampleTest(0);
    Test2 : Test_ExampleTest(0);
    Test3 : Test_ExampleTest(0);
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="Init" Id="{8a021792-fedf-418c-9fa3-308d88b77576}">
      <Declaration><![CDATA[METHOD Init
]]></Declaration>
      <Implementation>
        <ST><![CDATA[List.Clear();
List.AddTest(Test0);

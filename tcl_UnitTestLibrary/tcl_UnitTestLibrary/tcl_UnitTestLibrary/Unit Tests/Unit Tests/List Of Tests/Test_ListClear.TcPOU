List.AddTest(Test3);
]]></ST>
      </Implementation>
    </Method>
    <Method Name="RunTest" Id="{4404b0c7-fb1a-449c-948d-c29a698c43db}">
      <Declaration><![CDATA[METHOD RunTest
VAR_OUTPUT
    TestCompleted : BOOL;
    TestFailed    : BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[AssertTrue(Source    := TestName,
             Condition := List.Count = 4,
             Message   := 'List.Count was not 4',
             Failed    => TestFailed);
			 
List.Clear();			 

AssertTrue(Source    := TestName,
             Condition := List.Count = 0,
             Message   := 'List.Count was not 0',
             Failed    => TestFailed);

TestCompleted := TRUE;
]]></ST>
      </Implementation>
    </Method>
  </POU>

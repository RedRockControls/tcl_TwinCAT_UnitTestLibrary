    <Method Name="Init" Id="{c6c815cd-69fa-4977-953d-d0e1df3e2361}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD Init
VAR
    StringSplitter : StringSplitter;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[StringSplitter.Init(Str := InstanceName, Delim1 := '.', Delim2 := '');
WHILE StringSplitter.HasMore() DO
    TestName := StringSplitter.GetNext(); // to get last sub-section
END_WHILE

State         := 0;
TestFailed    := FALSE;
TestCompleted := FALSE;
]]></ST>
      </Implementation>
    </Method>

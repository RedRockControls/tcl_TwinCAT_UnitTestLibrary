    <Declaration><![CDATA[{attribute 'reflection'}
FUNCTION_BLOCK T_UnitTestBase IMPLEMENTS I_UnitTest
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
    {attribute 'instance-path'}
    {attribute 'no_init'}
    {attribute 'analysis' := '-33'} // Inhibit unused variable warning
    InstanceName : T_MaxString;     // Initialised with the instance name
    TestName     : T_MaxString;

    _TestCompleted : BOOL;
    _TestFailed    : BOOL;
    State          : INT;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Folder Name="Methods" Id="{a5772276-00c5-43a4-85c1-e780c7c7de3b}" />
    <Folder Name="Properties" Id="{eb7e9c1f-ae66-405a-820c-688b53e1a9a9}" />
    <Method Name="AssertAnyEquals" Id="{8c39ac6a-e3a4-4e64-8b71-f758c94e977f}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD AssertAnyEquals : BOOL
VAR_INPUT
    Expected : ANY;
    Actual   : ANY;
    Message  : T_MaxString;
END_VAR
VAR
    ComparisonResult : DINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[ComparisonResult := MEMCMP(pBuf1 := Expected.pValue,
                           pBuf2 := Actual.pValue,
                           n     := DINT_TO_UDINT(Actual.diSize));

IF Expected.TypeClass <> Actual.TypeClass THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;

ELSIF Expected.diSize <> Actual.diSize THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;

ELSIF ComparisonResult <> 0 THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;
END_IF

AssertAnyEquals := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AssertIsTrue" Id="{cad875ec-dfd3-4726-81dd-a6735f47baa6}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD AssertIsTrue : BOOL
VAR_INPUT
    Condition : BOOL;
    Message   : T_MaxString;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT Condition THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;
END_IF

AssertIsTrue := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AssertRealsAreEqual" Id="{0e4ee858-c05e-426c-a0eb-481c1ac21bf1}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD AssertRealsAreEqual : BOOL
VAR_INPUT
    Expected : LREAL;
    Actual   : LREAL;
    Delta    : LREAL;
    Message  : T_MaxString;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF ABS(Expected - Actual) > Delta THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;
END_IF

AssertRealsAreEqual := TRUE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AssertStringsAreEqual" Id="{922f5d37-eaf4-494b-9220-811e46507baa}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD AssertStringsAreEqual : BOOL
VAR_INPUT
    Expected : T_MaxString;
    Actual   : T_MaxString;
    Message   : T_MaxString;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Expected <> Actual THEN
    F_LogResult(TestName, FormatStringEx1('Assertion Failed - %s', Message));
    TestFailed := TRUE;
    RETURN;
END_IF

AssertStringsAreEqual := TRUE;
]]></ST>
      </Implementation>
    </Method>

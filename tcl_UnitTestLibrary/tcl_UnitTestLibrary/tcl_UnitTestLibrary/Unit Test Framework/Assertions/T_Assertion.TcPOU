    <Method Name="Assert" Id="{5e9acc7f-6ce2-451c-8a85-e51d1cf371f9}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD Assert
VAR_INPUT
    Caller           : T_MaxString;
    AssertionMessage : T_MaxString;
    Expected         : T_MaxString;
    Actual           : T_MaxString;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[_AssertionFailed  := TRUE;
_AssertionMessage := F_FormatStringEx2('Assertion failed in %s - %s', Caller, AssertionMessage);
_ExpectedAsString := F_FormatStringEx1('  Expected = %s', Expected);
_ActualAsString   := F_FormatStringEx1('  Actual   = %s', Actual);
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AssertEquals_BLOB" Id="{5eddac7b-ac1b-4f82-81db-408d888c0c87}" FolderPath="Methods\">
      <Declaration><![CDATA[METHOD AssertEquals_BLOB
VAR_INPUT
    Expected     : ANY;
    Actual       : ANY;
    VariableName : T_MaxString;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Expected.diSize <> Actual.diSize THEN
    _AssertionFailed  := TRUE;
    _AssertionMessage := F_FormatStringEx1('AssertEquals_BLOB failed in %s : Sizes differ', VariableName);

ELSIF Expected.TypeClass <> Actual.TypeClass THEN
    _AssertionFailed  := TRUE;
    _AssertionMessage := F_FormatStringEx1('AssertEquals_BLOB failed in %s: TypeClasses differ', VariableName);

ELSIF
    MEMCMP(pBuf1 := Expected.pValue, pBuf2 := Actual.pValue, n := DINT_TO_UDINT(Actual.diSize)) <> 0
THEN
    _AssertionFailed  := TRUE;
    _AssertionMessage := F_FormatStringEx1('AssertEquals_BLOB failed in %s: Contents differ', VariableName);
ELSE
    _AssertionFailed  := FALSE;
    _AssertionMessage := '';
END_IF
]]></ST>
      </Implementation>
    </Method>

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Expected <> Actual THEN
    _AssertionFailed  := TRUE;
    _AssertionMessage := F_FormatStringEx3('AssertEquals_BOOL failed : Variable "%s" =  %s, expected = %s', VariableName, BOOL_TO_STRING(Actual), BOOL_TO_STRING(Expected));
ELSE
    _AssertionFailed  := FALSE;
    _AssertionMessage := '';
END_IF]]></ST>
      </Implementation>
    </Method>
